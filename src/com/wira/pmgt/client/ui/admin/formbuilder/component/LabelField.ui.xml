<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:AbsolutePanel ui:field="container">
	        <g:HTMLPanel styleName="control-group">
		       <label ui:field="lblEl" class="control-label">Label:</label>
			    <div class="controls">
			    <g:InlineLabel ui:field="lblComponent" text="Label value">
			    </g:InlineLabel>
		    	</div>
	       </g:HTMLPanel>
	</g:AbsolutePanel>
</ui:UiBinder>