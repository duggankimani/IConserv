<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:d='urn:import:com.wira.pmgt.client.ui.component'>
	
	<g:AbsolutePanel ui:field="container">
	        <g:HTMLPanel styleName="control-group">
		       <label ui:field="lblEl" for="Username" class="control-label">Rating:</label>
			    <g:HTMLPanel styleName="controls" ui:field="panelControls">
				    <d:StarRating ui:field="ratingComponent" styleName="rating"/>
				    <g:InlineLabel ui:field="lblReadOnly"></g:InlineLabel>
		    	</g:HTMLPanel>
	       </g:HTMLPanel>
	</g:AbsolutePanel>
</ui:UiBinder>