<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:dt='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:d='urn:import:com.wira.pmgt.client.ui.component'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:FocusPanel ui:field="filterDialog">
		<g:HTMLPanel styleName="dialog dialog-filter is-visible" ui:field="divFilter">
			<g:HTMLPanel>
				<div class="pointer">
					<div class="arrow"></div>
					<div class="arrow_border"></div>
				</div>
				<div class="body">
					<div class="row-fluid">

						<p class="title span10">
							Filter Options:
						</p>
						<g:Anchor styleName="span2" ui:field="aClose">
							<i class="icon-remove pull-right" />
						</g:Anchor>
					</div>
					<div class="form">
						<div class="row-fluid">
							<span>By Progress:</span>
						</div>
						<div class="row-fluid">
							<d:DropDownList ui:field="lstDocType">
							</d:DropDownList>
						</div>
						<div class="row-fluid">
							<span>By Budget:</span>
						</div>
						<div class="row-fluid">
							<d:DropDownList ui:field="lstDocType2">
							</d:DropDownList>
						</div>
						<div class="row-fluid">
							<span>By Timelines:</span>
						</div>
						<div class="row-fluid">
							<d:DropDownList ui:field="lstDocType3">
							</d:DropDownList>
						</div>

						<div class="row-fluid">
							<span>By Targets:</span>
						</div>
						
						<div class="row-fluid">
							<d:DropDownList ui:field="lstDocType4">
							</d:DropDownList>
						</div>


						<div class="row-fluid">
							<g:Button styleName="btn btn-primary pull-left"
								ui:field="btnSearch">
								Search
							</g:Button>
						</div>
					</div>
				</div>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder>
