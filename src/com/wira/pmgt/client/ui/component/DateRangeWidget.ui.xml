<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	 xmlns:dt="urn:import:com.google.gwt.user.datepicker.client"
	 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<div class="row-fluid">
			<div class="span6">
				<div class="input-group">
					<dt:DateBox ui:field="dateInput1" styleName="form-control"></dt:DateBox>
					<g:InlineLabel styleName="input-group-addon"
						ui:field="spnCalendar1"></g:InlineLabel>
				</div>
			</div>
			<div class="span6">
				<div class="input-group">
					<dt:DateBox ui:field="dateInput2" styleName="form-control"></dt:DateBox>
					<g:InlineLabel styleName="input-group-addon"
						ui:field="spnCalendar2"></g:InlineLabel>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 