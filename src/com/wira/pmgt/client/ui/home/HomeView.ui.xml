<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:d='urn:import:com.wira.pmgt.client.ui.component'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="row-fluid main-container no-overflow-x">
		<g:HTMLPanel ui:field="mainContainer" styleName="full-page">
			<div class="span2 sidebar-nav hidden-phone" id="sidebar-nav">
				
				<!--Button Add Document -->
				<div class="add-doc">
					<g:Anchor ui:field="btnAdd" styleName="btn-danger btn-large">Create Program</g:Anchor>

					<!-- Document Pop-up -->
					<g:HTMLPanel ui:field="divDocPopup" styleName="dropdown-menu document-drop">
					</g:HTMLPanel>

				</div>

				<ul class="nav nav-tabs nav-stacked" id="dashboard-menu">
					<li>
						<g:Anchor href='#home;page=activities' ui:field='aActivities'>
							<i class="icon-tasks"></i>
							Activities
						</g:Anchor>
					</li>
					<li>
						<a href='#reports'>
							<i class="icon-bar-chart"></i>
							Reports
						</a>
					</li>
				</ul>
			</div>

			<g:HTMLPanel ui:field="activityContainer"
				styleName="content-right span10 no-overflow-y">
			</g:HTMLPanel>

		</g:HTMLPanel>
	</g:HTMLPanel>
	<!--End of Container -->
</ui:UiBinder>