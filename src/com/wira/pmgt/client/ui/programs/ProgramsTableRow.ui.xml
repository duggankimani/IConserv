<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.wira.pmgt.client.ui.component">
	<g:HTMLPanel styleName="tr" ui:field="row">
		<g:HTMLPanel styleName="td title" ui:field="divCheckbox"
			width="40%">
			<div class="row-fluid">
				<g:HTMLPanel styleName="span4 caret-container small-container">
					<d:ActionLink ui:field="aRowCaret"
						styleName="icon-caret-right helper-font-16 muted"></d:ActionLink>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="span4 strip-container small-container">
					<span class="color-strip-new" ui:field="divRowStrip"></span>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="span4 select-container small-container">
					<g:CheckBox ui:field="chkSelect"></g:CheckBox>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="span4 name-container">
					<g:HTMLPanel ui:field="popOverDiv" styleName="popover fade top in hide">
						<div class="arrow"></div>
						<h3 class="popover-title" ui:field="hTitle">Description</h3>
						<span ui:field="spnDescPopUp" class="popover-content">
						</span>
					</g:HTMLPanel>

					<d:ActionLink ui:field="aName" styleName=""></d:ActionLink>
					<div class="row-fluid">
						<g:InlineLabel ui:field="divDesc" styleName="muted italic helper-font-small"
							width="70%">
						</g:InlineLabel>
					</div>
				</g:HTMLPanel>
			</div>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="td" ui:field="divTimelines" width="10%">
			<g:HTMLPanel styleName="hide" ui:field="divDates">
				<div class="span5">
					<div class="helper-font-14 bold" ui:field="spanStartDay"></div>
					<div class="helper-font-14" ui:field="spanStartMonth"></div>
				</div>
				<div class="span2">
					<span class="icon-caret-right helper-font-18 muted"></span>
				</div>
				<div class="span5">
					<div class="helper-font-14 bold" ui:field="spanEndDay"></div>
					<div class="helper-font-14" ui:field="spanEndMonth"></div>
				</div>
			</g:HTMLPanel>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="td" ui:field="divStatus" width="10%">
			<span ui:field="spnStatus" class="label"></span>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="td" ui:field="divProgress" width="10%">
			<d:ProgressBar ui:field="progressBar"></d:ProgressBar>
			<!-- span ui:field="spnProgress"></span -->
		</g:HTMLPanel>

		<g:HTMLPanel styleName="td" ui:field="divBudget" title="Budget Amount"
			width="10%"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 