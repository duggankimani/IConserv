<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="row-fluid">
		
		<g:HTMLPanel styleName="span4 strip-container small-container">
			<span class="color-strip-new" ui:field="divRowStrip">
				<span ui:field="spnLabel"></span>
			</span>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="span4 select-container small-container">
			<g:CheckBox ui:field="chkSelect"></g:CheckBox>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="span4 name-container ellipsis">
			<span ui:field="spnName"></span>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 