<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="tr">
						<g:HTMLPanel styleName="center td">
							<label>
								<input type="checkbox"/>
							</label>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnName"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnDocTypes"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnFileName"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnProcessId"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnStatus"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnLastModified"></span>
						</g:HTMLPanel>
						
						<g:HTMLPanel styleName="td">
								<g:Anchor ui:field="aActivate" styleName="btn btn-default" title="Activate Process">
									<i class="icon-play"></i>
								</g:Anchor>
								<g:Anchor ui:field="aDeactivate" styleName="btn btn-default hide" title="De-activate Process">
									<i class="icon-minus-sign"></i>
								</g:Anchor>
								
								<g:Anchor ui:field="aRefresh" styleName="btn btn-default" title="Refresh Process">
									<i class="icon-undo"></i>
								</g:Anchor>
								<div class="btn-group">
									<button type="button" class="btn btn-default dropdown-toggle"
										data-toggle="dropdown">
										<i class="icon-cog"></i>
										<span class="caret"></span>
									</button>
									<ul class="dropdown-menu" role="menu">
										<li>
											<g:Anchor ui:field="aEdit">
												<i class="icon-edit"></i>
												Edit
											</g:Anchor>
										</li>
										<li>
											<g:Anchor ui:field="aDelete">
												<i class="icon-trash"></i>
												Delete
											</g:Anchor>
										</li>
									</ul>
								</div>
						</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>
