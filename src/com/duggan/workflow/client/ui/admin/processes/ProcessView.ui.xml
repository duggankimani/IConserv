<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
				<div class="content-body">
					<div class="row-fluid">
						<div class="action-buttons">
							<g:Anchor ui:field="aNewProcess" styleName="btn btn-primary"
								title="Add a New Process">
								<i class="icon-plus"></i>
								Add Process
							</g:Anchor>
					   </div>																			
					</div>
					
					<div class="table">
					    <div class="table-row">
							<div class="table-cell">
							  <label><input type="checkbox" class="ace"/></label>
							</div>
							<div class="table-cell">PROCESS NAME</div>
							<div class="table-cell">DOCUMENT TYPE</div>
							<div class="table-cell">FILE NAME</div>
							<div class="table-cell">PROCESS</div>
							<div class="table-cell">STATUS</div>
							<div class="table-cell">LAST MODIFIED</div>
							<div class="table-cell">ACTIONS</div>									
						</div>
						<g:HTMLPanel ui:field="tblRow" styleName="table-row-group">
						</g:HTMLPanel>
					</div>
				</div>
		</g:HTMLPanel>
</ui:UiBinder>
