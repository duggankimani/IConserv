<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component'
	xmlns:f='urn:import:com.duggan.workflow.client.ui.admin.formbuilder.component'
	xmlns:fb='urn:import:com.duggan.workflow.client.ui.admin.formbuilder'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

   	<g:AbsolutePanel ui:field="container">
	<g:HTMLPanel>
		<div class="content-body">
			<g:HTMLPanel styleName="palete-panel panel panel-default span2">
				<div class="panel-heading">
					<div class="row-fluid">
						<div class="span10">
						<h4>Paletes</h4>
						</div>
						<div class="span2">
							<g:Anchor styleName="minimize minimize-left"></g:Anchor>
						</div>
					</div>
				</div>
				<div class="panel-body">
						<ul class="nav nav-tabs">
					      <li class="active"><a href="#input" data-toggle="tab">Input</a></li>
					      <li><a href="#select" data-toggle="tab">Select</a></li>
					      <li><a href="#buttons" data-toggle="tab">Buttons</a></li>
					    </ul>
				    
				    	<div id="myTabContent" class="tab-content">
   						   <div class="tab-pane in" id="input">
							 <div class="form-horizontal">
								 <fieldset>
									<fb:PalettePanel ui:field="vTextInputPanel">	
									    <f:TextField></f:TextField>	
									    <f:SingleButton></f:SingleButton>
									</fb:PalettePanel>
			                      
									<fb:PalettePanel ui:field="vDatePanel">	
									    <f:DateField></f:DateField>					  	  
									</fb:PalettePanel>
									
								 	<fb:PalettePanel ui:field="vTextAreaPanel">	
									 	<f:TextArea></f:TextArea>
									</fb:PalettePanel>
									
									<fb:PalettePanel ui:field="vInlineRadioPanel">	
									 	<f:InlineRadio></f:InlineRadio>
									</fb:PalettePanel>
									
									
									<fb:PalettePanel ui:field="vInlineCheckBoxPanel">	
									 	<f:InlineCheckBox></f:InlineCheckBox>
									</fb:PalettePanel>
			                      </fieldset>
		                      </div>
	                      </div><!--End of Input Pane -->
	                      
	                      <div class="tab-pane in" id="select">
							 <div class="form-horizontal">
								 <fieldset>
									<fb:PalettePanel ui:field="vSelectBasicPanel">	
									    <f:SelectBasic></f:SelectBasic>	
									</fb:PalettePanel>
									
									<fb:PalettePanel ui:field="vSelectMultiplePanel">	
									    <f:SelectMultiple></f:SelectMultiple>	
									</fb:PalettePanel>
									
								</fieldset>
		                      </div>
	                      </div><!--End of Select Pane -->
	                      
	                      
	                       <div class="tab-pane in active" id="buttons">
							 <div class="form-horizontal">
								 <fieldset>
									<fb:PalettePanel ui:field="vSingleButtonPanel">	
									    <f:SingleButton></f:SingleButton>	
									    <f:DateField></f:DateField>	
									</fb:PalettePanel>
									
									<fb:PalettePanel ui:field="vMultipleButtonPanel">	
									    <f:MultipleButton></f:MultipleButton>	
									</fb:PalettePanel>
									
								</fieldset>
		                      </div>
	                      </div><!--End of Select Pane -->
	                      
	                   </div>
					</div>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="maincontent-panel panel panel-default span8">			
				<g:HTMLPanel styleName="panel-body">
					<g:HTMLPanel styleName="form-horizontal">
						<fieldset>
						 <legend>Form Name:</legend>
						</fieldset>
						
						<g:VerticalPanel ui:field="vPanel" styleName="span12">
						</g:VerticalPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
	</g:AbsolutePanel>
</ui:UiBinder>
