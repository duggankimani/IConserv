<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component'
	>
	<g:HTMLPanel>
		<g:HTMLPanel styleName="btn-group" ui:field="btnGroup">
			<div>
				<button type="button" class="btn btn-default dropdown-toggle"
					data-toggle="dropdown">
					<i class="icon-plus"></i>
					<span class="caret"></span>
				</button>
				<ul class="dropdown-menu" role="menu">
					<li>
						<d:ActionLink ui:field="atxtField">
							<i class="icon-edit"></i>
							TextField
						</d:ActionLink>
					</li>
					
					<li>
						<d:ActionLink ui:field="aRadioField">
							<i class="icon-edit"></i>
							Radio
						</d:ActionLink>
					</li>
					
					<li>
						<d:ActionLink ui:field="alblField">
							<i class="icon-edit"></i>
							Label
						</d:ActionLink>
					</li>
					
					<li>
						<d:ActionLink ui:field="adateBox">
							<i class="icon-edit"></i>
							Text Area
						</d:ActionLink>
					</li>
					
					<li>
						<d:ActionLink ui:field="achckBox">
							<i class="icon-edit"></i>
							CheckBox
						</d:ActionLink>
					</li>
					
					<li>
						<d:ActionLink ui:field="slctField">
							<i class="icon-edit"></i>
							Select Field
						</d:ActionLink>
					</li>
				</ul>
			
			</div>
	  </g:HTMLPanel>
		<div>
			<g:HorizontalPanel ui:field="hPanel" horizontalAlignment="center">
			</g:HorizontalPanel>			
		</div>
	</g:HTMLPanel>
</ui:UiBinder>