<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:AbsolutePanel ui:field="container">
	        <g:HTMLPanel styleName="control-group">
		       <label ui:field="lblEl" class="control-label">Label:</label>
			    <div class="controls">
			    <span ui:field="lblComponent" class="input-xlarge" text="Label value">
			        Sample value
			    </span>
		    	</div>
	       </g:HTMLPanel>
	</g:AbsolutePanel>
</ui:UiBinder>