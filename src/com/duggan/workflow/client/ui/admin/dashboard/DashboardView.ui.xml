<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'
	xmlns:c="urn:import:com.sencha.gxt.chart.client.chart">

	<g:HTMLPanel styleName="content-body overflow-y">

		<div class="row-fluid top-section section">
			<div class="span4">
				<a class="dashboard-stat tertiary">
					<div class="visual">
						<i class="icon-book"></i>
					</div> <!-- /.visual -->

					<div class="details">
						<span class="content-block">Requested Process</span>
						<span class="value">1,236</span>
					</div> <!-- /.details -->

					<i class="icon-play-circle more"></i>

				</a> <!-- /.dashboard-stat -->

			</div> <!-- /.col-md-3 -->

			<div class="span4">
				<a class="dashboard-stat secondary">
					<div class="visual">
						<i class="icon-ok"></i>
					</div> <!-- /.visual -->

					<div class="details">
						<span class="content-block">Active Processes</span>
						<span class="value">256</span>
					</div> <!-- /.details -->

					<i class="icon-play-circle more"></i>

				</a> <!-- /.dashboard-stat -->

			</div> <!-- /.col-md-3 -->

			<div class="span4">
				<a class="dashboard-stat primary">
					<div class="visual">
						<i class="icon-remove-circle"></i>
					</div> <!-- /.visual -->

					<div class="details">
						<span class="content-block">Failed Processes</span>
						<span class="value">0</span>
					</div> <!-- /.details -->

					<i class="icon-play-circle more"></i>

				</a> <!-- /.dashboard-stat -->

			</div> <!-- /.col-md-3 -->
		</div>

		<div class="row-fluid middle-section section">
			<div class="span6">
				<div class="portlet">
					<div class="portlet-header">
						<h3>
							<i class="icon-bar-chart"></i>
							AGING ANALYSIS
						</h3>
					</div>
					<g:HTMLPanel styleName="portlet-content" ui:field="panelTurnAroundTime">
					</g:HTMLPanel>
				</div>
			</div>

			<div class="span6">
				<div class="portlet">
					<div class="portlet-header">
						<h3>
							<i class="icon-bar-chart"></i>
							TOTAL REQUESTS MADE PER DOCUMENT
						</h3>
					</div>
					<g:HTMLPanel styleName="portlet-content" ui:field="panelTotalRequestsPerDoc">
					</g:HTMLPanel>
				</div>
			</div>
		</div>

		<div class="row-fluid middle-section section">
			<div class="portlet">
				<div class="portlet-header">
					<h3>
						<i class="icon-bar-chart"></i>
						LONGEST WAIT POINTS
					</h3>
				</div>
				<div class="portlet-content">
					<div class="table table-striped table-hover table-bordered">
						<div class='thead'>
							<div class="tr">
								<div class='th'>#</div>
								<div class='th'>PROCESS ITEM</div>
								<div class='th'>DOCUMENT TYPE</div>
								<div class='th'>PROCESS TIME</div>
							</div>
						</div>

						<div class='tbody'>
							<div class="tr">
								<div class='td'>1.</div>
								<div class='td'>HOD PROCUREMENT APPROVAL</div>
								<div class='td'>INVOICE</div>
								<div class='td'>
									<span class="label label-default arrowed-in">14 Days</span>
								</div>
							</div>

							<div class="tr">
								<div class='td'>2.</div>
								<div class='td'>CEO APPROVAL</div>
								<div class='td'>LPO</div>
								<div class='td'>
									<span class="label label-warning arrowed-in">2 Weeks</span>
								</div>
							</div>

							<div class="tr">
								<div class='td'>3.</div>
								<div class='td'>MANAGER APPROVAL</div>
								<div class='td'>LPO</div>
								<div class='td'>
									<span class="label label-info arrowed-in">1 Day</span>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="row-fluid middle-section section">
			<div class="portlet">
				<div class="portlet-header">
					<h3>
						<i class="icon-bar-chart"></i>
						REQUESTS VS APPROVALS CHART
					</h3>
				</div>
				<g:HTMLPanel styleName="portlet-content" ui:field="panelRequestsApprovalsComp">
				</g:HTMLPanel>
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder>
