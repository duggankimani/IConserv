<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="table-row">
						<g:HTMLPanel styleName="center table-cell">
							<label>
								<input type="checkbox"/>
							</label>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="table-cell"><div style="width: 50px" >Invoice</div></g:HTMLPanel>
						<g:HTMLPanel styleName="table-cell">Invoice_process.bpmn</g:HTMLPanel>
						<g:HTMLPanel styleName="table-cell">Active</g:HTMLPanel>
						<g:HTMLPanel styleName="table-cell">22/9/2013</g:HTMLPanel>
						<g:HTMLPanel styleName="table-cell">
								<g:Anchor ui:field="aActivate" styleName="btn btn-default" title="Activate Process">
									<i class="icon-play"></i>
								</g:Anchor>
								<g:Anchor ui:field="aDeactivate" styleName="btn btn-default" title="De-activate Process">
									<i class="icon-minus-sign"></i>
								</g:Anchor>
								<g:Anchor ui:field="aEdit" styleName="btn btn-default" title="Edit">
									<i class="icon-edit"></i>
								</g:Anchor>
								<g:Anchor ui:field="aDelete" styleName="btn btn-default" title="Delete">
									<i class="icon-trash"></i>
								</g:Anchor>
						</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>
