<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:up='urn:import:com.duggan.workflow.client.ui.upload.custom'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:DialogBox styleName="modal" modal="true" width="100%" ui:field="uploaderDialog">
		<g:HTMLPanel>
			<g:HTMLPanel styleName="modal-header">
					<h5><i class="icon-arrow-up"></i>Upload Documents</h5>
			 </g:HTMLPanel>
				
				<g:HTMLPanel ui:field="uploadPanel" styleName="uploadPanel">
					<up:Uploader ui:field="uploader"></up:Uploader>
				</g:HTMLPanel>
	
			<g:HTMLPanel styleName="modal-footer">
				<g:Button styleName="btn btn-primary pull-right" ui:field="btnDone"
					text="DONE" />
				<g:Button styleName="btn btn-default pull-left" ui:field="btnCancel"
					text="CANCEL" />
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder>
