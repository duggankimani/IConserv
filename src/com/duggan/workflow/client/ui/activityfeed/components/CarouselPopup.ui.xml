<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:com.duggan.workflow.client.ui.component">

	<g:FocusPanel ui:field="panelContainer">
		<g:HTMLPanel>
			<div class="carousel">
				<div class="row-fluid">
					<div class='offset-2 span12 text-center'>
						<h2><span ui:field="spnHeader">Creating New Request</span></h2>
					</div>
				</div>
				<div class='row-fluid'>
					<div class='offset-2 span12'>
						<div class="carousel" data-ride="carousel" data-interval="false"
							id="quote-carousel">
							<!-- Bottom Indicators -->
							<p:OLPanel styleName="carousel-indicators" ui:field="olCarousels">
							</p:OLPanel>

							<!-- Inner Items Carousel -->
							<g:HTMLPanel styleName="carousel-inner" ui:field="innerCarousel">
							</g:HTMLPanel>


							<!-- Carousel Buttons Next/Prev -->
							<a data-slide="prev" href="#quote-carousel" class="left carousel-control">
								<i class="icon-chevron-left"></i>
							</a>
							<a data-slide="next" href="#quote-carousel" class="right carousel-control">
								<i class="icon-chevron-right"></i>
							</a>
						</div>
					</div>
				</div>
			</div>
		</g:HTMLPanel>

	</g:FocusPanel>
</ui:UiBinder> 